<style>

svg path {
    transition: fill .15s ease-in;
}

a:hover svg path{
    fill: var(--purple-200);
}

h3 {
    font-size: 24px;
    text-transform: uppercase;
}
li {
    padding: 4px 0;
    transition: .15s ease-in;
}

#information li {
    cursor: pointer;
    text-transform: uppercase;
}
#information li:hover {
color: var(--purple-200);
}


#footer div {
    display: flex;
    flex-direction: column;
    gap: 16px;
}

#information ul {
    display: grid;
    grid-template-columns: 1fr 1fr;
    column-gap: 48px;
}

#socials .icons, #contact .icons {
    display: flex;
    flex-direction: row;
    gap: 48px;
}

@media (min-width: 900px) {
    .mobile {
        display: none !important;
    }
    li {
    font-size: 16px;
}
#information {
    border-left: solid 1px var(--stroke-light);
    border-right: solid 1px var(--stroke-light);
    padding: 0 80px;
}
#footer {
    max-width: 1120px;
    margin: auto;
    display: flex;
    flex-direction: row;
    padding: 32px 0;
    justify-content: space-between;
}

}
@media (max-width: 899px) {
    .desktop {
        background-color: red;
        display: none !important;
    }
    #footer {
    display: flex;
    flex-direction: column;
    padding: 24px 32px;
}
#contact, #information {
    padding: 24px 0;
}
#contact .icons {
gap: 24px;
}

#information {
    border-top: solid 1px var(--stroke-light);
}
li {
    font-size: 14px;
}
svg { 
    width: 24px;
}
}

</style>